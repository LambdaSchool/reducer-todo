yload)\n        //3. return a copy of the item with completed flipped\n        // if todo does not have id we are looking for, return the item as is\n            return({...state, todos:state.todos.map(todo=>{\n                if (todo.id === action.payload) {\n                    return ({...todo, completed: !todo.completed})\n                }\n                else {\n                    return(todo)\n                }\n            }) });\n        case(\"CLEAR_COMPLETE\"):\n        // Return an array of todos where todo.completed is false\n            return({...state, todos:state.todos.filter(todo=>{\n                    return (!todo.completed)\n                \n            })})\n            return({})\n        default:\n            return(state)\n    }\n}\n\nexport default reducer","/Users/loustevens/Desktop/Lambda/Sprint3-Adavnced-React/Advanced-State-Management/reducer-todo/todo/src/actions/index.js",[],"/Users/loustevens/Desktop/Lambda/Sprint3-Adavnced-React/Advanced-State-Management/reducer-todo/todo/src/components/TodoList.js",[],"/Users/loustevens/Desktop/Lambda/Sprint3-Adavnced-React/Advanced-State-Management/reducer-todo/todo/src/components/TodoForm.js",[],"/Users/loustevens/Desktop/Lambda/Sprint3-Adavnced-React/Advanced-State-Management/reducer-todo/todo/src/components/Todo.js",[],{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","severity":1,"message":"46","line":1,"column":17,"nodeType":"47","messageId":"48","endLine":1,"endColumn":25},{"ruleId":"45","severity":1,"message":"49","line":2,"column":33,"nodeType":"47","messageId":"48","endLine":2,"endColumn":38},{"ruleId":"50","severity":1,"message":"51","line":46,"column":13,"nodeType":"52","messageId":"53","endLine":46,"endColumn":23},"no-native-reassign",["54"],"no-negated-in-lhs",["55"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'todos' is defined but never used.","no-unreachable","Unreachable code.","ReturnStatement","unreacha